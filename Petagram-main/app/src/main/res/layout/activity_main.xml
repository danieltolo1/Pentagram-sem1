<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="1140dp"
        android:layout_height="1689dp"
        android:background="@color/primary_c"
        android:contentDescription="@string/todo"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/coursera_logo" />

    <Button
        android:id="@+id/button"
        android:layout_width="775dp"
        android:layout_height="120dp"
        android:text="VETERINARIA"
        android:textColor="@color/white"
        android:textSize="40sp"
        app:backgroundTint="@color/accent_c"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="395dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="1045dp"
        android:layout_height="1070dp"
        app:srcCompat="@drawable/coursera_curso3sem1"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="550dp" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>